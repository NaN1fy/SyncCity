          "pluginVersion": "4.0.7",
          "queryType": "table",
          "rawSql": "SELECT t3.sensor_name as Sensor, t4.avg as Mean, t3.mode as Mode, t4.min as Min, t4.max as Max\r\nFROM\r\n(SELECT t1.sensor_name as sensor_name, t2.mode as mode\r\nFROM \r\n(SELECT sensor_name, max(count) as count\r\nFROM\r\n(SELECT sensor_name, readings as mode, COUNT(*) as count\r\nFROM  sc_database.temperature\r\nGROUP BY readings, sensor_name\r\nORDER BY count(*) desc)\r\nGROUP BY sensor_name) t1\r\nLEFT JOIN\r\n(\r\n  SELECT sensor_name, readings as mode, COUNT(*) as count\r\n  FROM  sc_database.temperature\r\n  GROUP BY readings, sensor_name\r\n  ORDER BY count(*) desc\r\n) t2\r\nON t2.count = t1.count) t3\r\nLEFT JOIN\r\n(SELECT sensor_name, AVG(readings) as avg, MIN(readings) as min, MAX(readings) as max\r\nfrom sc_database.temperature group by sensor_name) t4\r\nON t3.sensor_name = t4.sensor_name",
          "refId": "A"
        }
